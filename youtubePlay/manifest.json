{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "youtube to airplay"
  },
  "content_scripts": [
    {
      "all_frames": false,
      "js": [
        "js/script.js"
      ],
      "matches": [
        "*://www.youtube.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "This extension allows sending YouTube videos to AppleTV via AirPlay",
  "icons": {
    "128": "icon.png",
    "16": "icon.png",
    "48": "icon.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "manifest_version": 2,
  "name": "youtubePlay",
  "permissions": [
    "activeTab",
    "background",
    "storage",
    "*://www.youtube.com/*",
    "http://*.local:7000/*",
    "webRequest",
    "webRequestBlocking"
  ],
  "version": "1.0.0"
}
